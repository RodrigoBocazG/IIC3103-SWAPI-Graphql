<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Release date</th>
      <th>Director</th>
      <th>Producer(s)</th>
      <th>Episode number</th>
    </tr>
  </thead>
  <tbody>
    <% @movies.each do |movie|%>
      <tr>
        <td><%= movie.title %></td>
        <td><%= movie.release_date %></td>
        <td><%= movie.director %></td>
        <td><%= movie.producers %></td>
        <td><%= movie.episode_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

